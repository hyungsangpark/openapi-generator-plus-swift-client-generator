{
  "name": "@openapi-generator-plus/swift-client-generator",
  "version": "0.1.3",
  "description": "An OpenAPI Generator Plus template for a Swift API client",
  "keywords": [
    "openapi-generator-plus",
    "openapi-generator-plus-generator",
    "openapi",
    "openapi-generator",
    "swift",
    "client"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "rimraf dist",
    "link-openapi-generator-plus": "pnpm link --global openapi-generator-plus $(cd node_modules && find @openapi-generator-plus -depth 1 -prune)",
    "release": "pnpm clean && pnpm build && pnpm test && pnpx changeset publish && pnpm install",
    "release:version": "pnpm changeset version && pnpm install",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "author": "Karl von Randow, Tom Carey",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/karlvr/openapi-generator-plus-swift-client-generator/issues"
  },
  "homepage": "https://github.com/karlvr/openapi-generator-plus-swift-client-generator#readme",
  "dependencies": {
    "@openapi-generator-plus/core": "^0.41.3",
    "@openapi-generator-plus/generator-common": "^0.25.1",
    "@openapi-generator-plus/handlebars-templates": "^0.27.0",
    "@openapi-generator-plus/java-like-generator-helper": "^0.16.3",
    "@openapi-generator-plus/testing": "^0.41.0",
    "@openapi-generator-plus/types": "^0.41.2",
    "handlebars": "^4.7.7",
    "openapi-generator-plus": "^0.41.2"
  },
  "peerDependencies": {
    "@openapi-generator-plus/core": "^0.41.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.17.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.0.5",
    "jest": "^27.2.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlvr/openapi-generator-plus-swift-client-generator.git"
  }
}
