import Foundation

public enum APIError: Error {
    /// The authentication could not be attempted
    case authenticationPrevented

    /// The authentication request failed
    case authenticationFailed(_ response: HTTPURLResponse, data: Data)

    case invalidResponse(_ error: Error, response: HTTPURLResponse, data: Data)

    case systemError(_ error: Error)

    case securitySchemeNotConfigured(_ securityScheme: SecurityScheme)
    case unexpectedResponse(_ response: URLResponse, data: Data)
}
